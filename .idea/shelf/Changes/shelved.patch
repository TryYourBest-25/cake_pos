+      console.error('L·ªói fetch profile:', error);
+      set({ 
+        error: 'Kh√¥ng th·ªÉ t·∫£i th√¥ng tin profile', 
+        isLoading: false 
+      });
+      throw error;
+    }
+  },
+
+  updateProfile: async (data: UpdateProfileDto) => {
+    try {
+      set({ isUpdating: true, error: null });
