+  return new Intl.NumberFormat('vi-VN', {
+    style: 'currency',
+    currency: 'VND',
+  }).format(value);
+}
+
+function formatChartData(data: ProductSalesData[], showTop: number = 10) {
+  return data
+    .slice(0, showTop)
+    .map(item => ({
+      name: item.product_name.length > 20 
