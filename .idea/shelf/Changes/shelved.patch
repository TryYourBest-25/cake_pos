+        : item.product_name,
+      fullName: item.product_name,
+      quantity: item.quantity_sold,
+      revenue: item.revenue,
+      value: item.quantity_sold, // For pie chart
+    }));
+}
+
+export function ProductSalesChart({ 
+  data, 
+  type, 
+  title, 
+  description, 
+  showTop = 10 
+}: ProductSalesChartProps) {
